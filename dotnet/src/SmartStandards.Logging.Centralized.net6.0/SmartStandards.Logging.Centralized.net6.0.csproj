<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>SmartStandards.Logging.Centralized</AssemblyName>
    <TargetFramework>net6.0</TargetFramework>
    <OutputPath>..\..\dist\bin-net6.0\</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DocumentationFile>..\..\dist\bin-net6.0\SmartStandards.Logging.Centralized.xml</DocumentationFile>
    <RootNamespace>Logging</RootNamespace>
